{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "nodejs18.x"
        }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/app.js",
        "methods": ["GET"]
      },
      {
        "src": "/auth/(.*)",
        "dest": "/app.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      },
      {
        "src": "/wallet/(.*)",
        "dest": "/app.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "app.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  