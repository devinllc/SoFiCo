{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/app.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/wallet/(.*)",
        "dest": "/app.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  